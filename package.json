{
  "name": "com.afklm.tecc-se.openflight.frontend",
  "description": "Angular frontend for workshop about reactive streams using OpenFlight data in a reactive backend to plot markers and update their data accordingly. June 2018.",
  "version": "0.0.1",
  "engines": {
    "node": "^8.11.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "node mock-app",
    "dev": "ng serve",
    "dev:mock": "ng serve --proxy-config proxy.conf.json",
    "dev:backend": "ng serve --proxy-config proxy-backend.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration=production --prod --stats-json",
    "test": "ng test --stats-json",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.4",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/googlemaps": "^3.30.9",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-angular": "^1.1.2",
    "typescript": "~2.7.2"
  }
}
